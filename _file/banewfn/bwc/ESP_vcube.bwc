wfn=*.fchk
[grid]
%process
    electron
    esp
%command
#!/bin/bash
    mkdir -p ESP
    mv density.cub ESP/${input}_den.cub
    mv totesp.cub ESP/${input}_esp.cub

    cat << EOF > ESP/esp.bat
    vmd -e esp.vmd
EOF

    cat << EOF2 > ESP/esp.vmd
    vcube *_den.cub map *_esp.cub
    set colorlow -20
    set colorhigh 20
    mol scaleminmax 0 1 -20 20
    puts "unit: kcal/mol"
EOF2
end


