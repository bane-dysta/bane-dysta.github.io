wfn=*.fchk
[fmo]
%process
    orb index h
    orb index l
%command
#!/bin/bash
mv h.cub ${input}_oH.cub
mv l.cub ${input}_oL.cub
cat << EOF > fmo.bat
vmd -e fmo.vmd
EOF
cat << EOF2 > fmo.vmd
vcube *.cub
EOF2
end

